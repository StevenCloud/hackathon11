{
    "name" : "Page Preview alex3's",
    "version": "1.0",
    "manifest_version": 2,
    "background":{
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
        
    ],
    "browser_action": {
        "default_icon": {
            "48": "Images/bucc.png"
        }
    },

    "icons": {
        "48": "Images/bucc.png"
    },

    "permissions": ["storage"],
    "browser_action": {
        "default_popup": "popup.html"
    }

}